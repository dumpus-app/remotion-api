{
  "name": "remotion-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "server:build": "nest build",
    "server:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "server:start": "nest start",
    "server:start:dev": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:start:prod": "node dist/src/main",
    "lint": "eslint \"{src,video,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./test/jest-e2e.json",
    "remotion:start": "remotion preview video/index.ts",
    "remotion:build": "remotion render video/index.ts Video out/video.mp4",
    "remotion:upgrade": "remotion upgrade",
    "remotion:test": "eslint video --ext ts,tsx,js,jsx && tsc video/index.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml --env-file .env up"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/throttler": "^4.0.0",
    "@remotion/bundler": "^3.3.98",
    "@remotion/cli": "^3.3.98",
    "@remotion/google-fonts": "^3.3.98",
    "@remotion/renderer": "^3.3.98",
    "@remotion/tailwind": "^3.3.98",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "remotion": "^3.3.98",
    "rxjs": "^7.2.0",
    "tailwindcss": "^3.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/react": "^18.2.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@types/web": "^0.0.99",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
